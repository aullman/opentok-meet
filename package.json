{
  "name": "opentok-meet",
  "description": "Opentok app with screen sharing using the WebRTC screen sharing feature.",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "angular": "^1.5.5",
    "ejs": ">= 0.0.1",
    "express": "3.1.x",
    "hiredis": "~0.4.1",
    "jquery": "^2.2.3",
    "opentok": "^2.2.3",
    "opentok-angular": "^1.1.0",
    "opentok-editor": "^0.3.0",
    "opentok-whiteboard": "^1.1.0",
    "redis": "~0.10.1",
    "webpack": "^1.13.0",
    "style-loader": "^0.13.1",
    "css-loader": "^0.23.1"
  },
  "engines": {
    "node": "4.2.2",
    "npm": "2.14.7"
  },
  "scripts": {
